{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps",
  "projects": {
    "job-crawler": {
      "type": "application",
      "root": "apps/job-crawler",
      "entryFile": "main",
      "sourceRoot": "apps/job-crawler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/job-crawler/tsconfig.json"
      }
    },
    "web-service": {
      "type": "application",
      "root": "apps/web-service",
      "entryFile": "main",
      "sourceRoot": "apps/web-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/web-service/tsconfig.json"
      }
    }
  }
}
